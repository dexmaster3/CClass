#include <stdio.h>

int main(void)
{
    char chr;
    int num;

    printf("Enter some text, equal sign and an integer:\n");
    while ((chr = getchar()) != '=')
    {
        continue;
    }
    scanf("%d", &num);
    printf("The integer you entered is: %d\n", num);

    return 0;
}
